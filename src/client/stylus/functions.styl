inline-block()
	box-sizing border-box
	display inline-block
	vertical-align top

centerContent()
	display flex
	align-items center
	justify-content center

label()
	color #999
	font-size 0.8em
	letter-spacing 1px
	margin-bottom 0.4em
	text-transform uppercase

well()
	background-color white
	box-shadow 1px 1px 3px rgba(0, 0, 0, 0.5)
	box-sizing border-box
	padding 5%
